<% @results %>

<table class="table">
		<tr>
			<td> Имя </td>
			<td> Телефон </td>
		</tr>

		<% @results.each do |row| %>

		<tr>
			<td> <%= row['Name'] %> </td>
			<td> <%= row['Phone'] %> </td>
			<td> <%= row['DateStamp'] %> </td>
			<td> <%= row['Barber'] %> </td>
			<td> <%= row['Color'] %> </td>
		</tr>
	<% end %>
</table>
